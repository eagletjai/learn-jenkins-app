{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "jash-learnjenkins-taskdef-prod",
    "containerDefinitions": [
        {
            "name": "learnjenkins",
            "image": "054301581865.dkr.ecr.ap-southeast-2.amazonaws.com/learnjenkinsapp:#APP_VERSION#",
            "portMappings": [{
                "name": "ngnix-80-tcp",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::054301581865:role/ecsTaskExecutionRole"
}